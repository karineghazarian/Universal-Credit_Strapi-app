      "default": "/"
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "component.slider",
        "component.cards",
        "component.calculator",
        "component.markdown",
        "component.map",
        "component.modal-warning",
        "component.warning",
        "component.application",
        "component.monthly-report",
        "component.yearly-report",
        "component.terms",
        "component.rules",
        "component.files"
      ],
      "required": true
    }
  }
}
